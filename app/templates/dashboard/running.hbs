<div class="dashboard-repos">
  <section class="dashboard-section dashboard-starred">
    <h2 class="small-title">Starred repositories</h2>
    <ul class="repo-list">
      {{#each model.starredRepos as |repo|}}
        {{dashboard-row repo=repo star=star unstar=unstar}}
      {{else}}
        <div class="starred-empty">
          {{svg-jar 'icon-star' class="icon--big"}} You can keep track of your favorite repositories here once you start starring!
        </div>
      {{/each}}
    </ul>
  </section>

  <section class="dashboard-section dashboard-active">
    <h2 class="small-title">Currently running jobs</h2>
    <ul class="repo-list">
      {{#each model.runningJobs as |job|}}
        <li>
          {{job.state}}
          {{job.number}}
          {{job.startedAt}}
          {{job.repo.slug}}
        </li>
      {{else}}
        {{no-repos}}
      {{/each}}
    </ul>
  </section>
</div>
